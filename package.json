{
  "name": "arbstake",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 8080 --turbo",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "compile-contracts": "cd web3 && npx hardhat compile && node scripts/export-abi.js",
    "sync-abi": "node scripts/sync-abi.cjs",
    "check-contract": "npx tsx scripts/check-contract.ts",
    "check-adapter": "npx tsx scripts/check-adapter.ts",
    "simulate-deposit": "npx tsx scripts/simulate-deposit.ts",
    "diagnose": "npm run check-contract && npm run check-adapter && npm run simulate-deposit"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.5",
    "next": "15.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "viem": "^2.38.4",
    "wagmi": "^2.19.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
